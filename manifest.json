{
    "manifest_version": 2,
    "name": "Greenify",
    "version": "0.0.1",
    "permissions": [
      "activeTab",
      "webRequest",
      "webRequestBlocking",
      "background",
      "<all_urls>",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon":"assets/logo.png"
    }
  }